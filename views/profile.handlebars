<div class="card">
    <div class="card_background_img">
    </div>
    <div class="card_profile_img">
      <input type="file" class="file" accept="image/png", "image/jpeg", "image/jpg">
    </div>

    
    <div class="user_details">
        <h2>Full Name:</h2>
        <h3 id="myname">{{ name }}</h3>
    </div>
    <div class="user_detail">
        <h2>Email:</h2>
        <h3 id="myemail">{{ email }}</h3>    
    </div>
  <form class="profileform">
     <div class="user_number">
        <h2>Phone:</h2>
        <input name="number" class="usr" type="text"></input>   
        <h3 class="usrdata"></h3>
    </div>
     <div class="user_wrkout">
        <h2>Favorite Workout:</h2>
        <input name="Workout" class="usr" id="input1" type="text"></input>   
        <h3 class="usrdata2"></h3>
    </div>
      <div class="user_diet">
        <h2>Favorite Diet:</h2>
        <input name="Diet" class="usr" id="input2" type="text"></input>   
        <h3 class="usrdata3"></h3>
    </div>
      <div class="user_diet">
        <h2>Relationship Status:</h2>
        <input name="Status" class="usr" id="input3" type="text"><button class="usrbtn">submit</button>  
        <h3 class="usrdata4"></h3>
    </div>
</div>
</form>





    


